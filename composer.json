{
    "name": "xi/fixtures",
    "type": "library",
    "description": "Convenient creation of Doctrine entities in tests. Like Ruby's FactoryGirl.",
    "keywords": ["doctrine", "fixture", "test", "factory", "factorygirl"],
    "homepage": "http://github.com/xi-project/xi-fixtures",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "xi-project",
            "email": "xi.devs@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "doctrine/orm": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "Xi\\Fixtures": "xi-fixtures/library/"
        }
    },

    "target-dir": "xi-fixtures"
}
